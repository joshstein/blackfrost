<UserControl x:Class="Blackfrost.Views.PvPClassicPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="summoner's rift" Source="/Views/GamePages/PvP/SRPvPTypes.xaml"/>
                <mui:Link DisplayName="twisted treeline" Source="/Views/GamePages/PvP/TTPvPTypes.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>